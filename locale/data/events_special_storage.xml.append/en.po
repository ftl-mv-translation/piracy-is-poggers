#: src-en/data/events_special_storage.xml.append:392
msgid ""
"data/events_special_storage.xml.append$//mod:findName[@name=\"STORAGE_CHECK_BUTTON\"]/mod-"
"append:choice[1]/text"
msgstr "Use the Mercenary Transmitter."

#: src-en/data/events_special_storage.xml.append:394
msgid ""
"data/events_special_storage.xml.append$//mod:findName[@name=\"STORAGE_CHECK_BUTTON\"]/mod-"
"append:choice[1]/event/text"
msgstr ""
"You ponder which service you want to order today. (You will only get a "
"reputation increase if you use the transmitter two times within the same "
"sector)"

#: src-en/data/events_special_storage.xml.append:396
