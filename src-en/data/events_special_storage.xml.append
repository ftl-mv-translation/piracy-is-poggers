	<mod:findName type="event" name="COMBAT_CHECK_REAL">
		<mod:findWithChildLike type="choice" child-type="text">
			<mod:selector>Do nothing.</mod:selector>
			<mod:removeTag/>
		</mod:findWithChildLike>
		<mod:findWithChildLike type="choice" child-type="text">
			<mod:selector>Change your toggle or amp equipment.</mod:selector>
			<mod:removeTag/>
		</mod:findWithChildLike>
		<mod:findWithChildLike type="choice" child-type="text">
			<mod:selector>Do some last minute check-ups.</mod:selector>
			<mod:removeTag/>
		</mod:findWithChildLike>
		<!--Prototype Combat Augments-->
		<mod-append:choice req="PIP_COMBAT_RELOADER" hidden="true" blue="false">
			<text>(Accelerated Reloader) Activate the reloader!</text>
			<event load="PIP_COMBAT_CHECK_RELOADER"/>
		</mod-append:choice>
		<!--Mini Payloads-->
		<mod-append:choice req="PIP_COMBAT_EXPLOSIVE_MULTIPLE_5" hidden="true" blue="false">
			<text>(Mini Payloads) Deploy a mini payload. [5 remaining]</text>
			<event load="PIP_COMBAT_CHECK_EXPLOSIVE_MULTIPLE_5"/>
		</mod-append:choice>
		<mod-append:choice req="PIP_COMBAT_EXPLOSIVE_MULTIPLE_4" hidden="true" blue="false">
			<text>(Mini Payloads) Deploy a mini payload. [4 remaining]</text>
			<event load="PIP_COMBAT_CHECK_EXPLOSIVE_MULTIPLE_4"/>
		</mod-append:choice>
		<mod-append:choice req="PIP_COMBAT_EXPLOSIVE_MULTIPLE_3" hidden="true" blue="false">
			<text>(Mini Payloads) Deploy a mini payload. [3 remaining]</text>
			<event load="PIP_COMBAT_CHECK_EXPLOSIVE_MULTIPLE_3"/>
		</mod-append:choice>
		<mod-append:choice req="PIP_COMBAT_EXPLOSIVE_MULTIPLE_2" hidden="true" blue="false">
			<text>(Mini Payloads) Deploy a mini payload. [2 remaining]</text>
			<event load="PIP_COMBAT_CHECK_EXPLOSIVE_MULTIPLE_2"/>
		</mod-append:choice>
		<mod-append:choice req="PIP_COMBAT_EXPLOSIVE_MULTIPLE_1" hidden="true" blue="false">
			<text>(Mini Payloads) Deploy a mini payload. [1 remaining, will deplete augment after use]</text>
			<event load="PIP_COMBAT_CHECK_EXPLOSIVE_MULTIPLE_1"/>
		</mod-append:choice>
		<mod-append:choice req="LIST_CHECK_TOGGLES_AND_AMPS" lvl="1" blue="false">
			<text>Change your toggle or amp equipment.</text>
